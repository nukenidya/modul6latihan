<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectenam">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="ContactList"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.projectenam.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.projectenam.PhoneDetailActivity"
            android:parentActivityName="com.example.projectenam.MainActivity">
        </activity>
    </application>

</manifest>